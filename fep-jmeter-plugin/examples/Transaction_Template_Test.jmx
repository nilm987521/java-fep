<?xml version="1.0" encoding="UTF-8"?>
<jmeterTestPlan version="1.2" properties="5.0" jmeter="5.6.3">
  <hashTree>
    <TestPlan guiclass="TestPlanGui" testclass="TestPlan" testname="Transaction Template Test Plan" enabled="true">
      <boolProp name="TestPlan.functional_mode">false</boolProp>
      <boolProp name="TestPlan.serialize_threadgroups">false</boolProp>
      <stringProp name="TestPlan.comments">Demonstrates the use of TransactionTemplate and TemplateSampler for flexible ISO 8583 message generation.</stringProp>
      <elementProp name="TestPlan.user_defined_variables" elementType="Arguments" guiclass="ArgumentsPanel" testclass="Arguments" testname="User Defined Variables" enabled="true">
        <collectionProp name="Arguments.arguments">
          <elementProp name="fep_host" elementType="Argument">
            <stringProp name="Argument.name">fep_host</stringProp>
            <stringProp name="Argument.value">localhost</stringProp>
            <stringProp name="Argument.metadata">=</stringProp>
          </elementProp>
          <elementProp name="fep_port" elementType="Argument">
            <stringProp name="Argument.name">fep_port</stringProp>
            <stringProp name="Argument.value">8080</stringProp>
            <stringProp name="Argument.metadata">=</stringProp>
          </elementProp>
        </collectionProp>
      </elementProp>
    </TestPlan>
    <hashTree>
      <!-- Transaction Template Config (Shared) -->
      <com.fep.jmeter.config.TransactionTemplateConfig guiclass="TestBeanGUI" testclass="com.fep.jmeter.config.TransactionTemplateConfig" testname="Transaction Template Config" enabled="true">
        <stringProp name="templateSource">COMMON</stringProp>
        <stringProp name="templateFile"></stringProp>
        <stringProp name="inlineTemplates"></stringProp>
        <boolProp name="useCommonTemplates">true</boolProp>
        <boolProp name="autoGenerateStan">true</boolProp>
        <boolProp name="autoGenerateTimestamp">true</boolProp>
      </com.fep.jmeter.config.TransactionTemplateConfig>
      <hashTree/>

      <!-- Common Templates Thread Group -->
      <ThreadGroup guiclass="ThreadGroupGui" testclass="ThreadGroup" testname="Common Templates Test" enabled="true">
        <stringProp name="ThreadGroup.on_sample_error">continue</stringProp>
        <elementProp name="ThreadGroup.main_controller" elementType="LoopController" guiclass="LoopControlPanel" testclass="LoopController" testname="Loop Controller" enabled="true">
          <stringProp name="LoopController.loops">1</stringProp>
          <boolProp name="LoopController.continue_forever">false</boolProp>
        </elementProp>
        <stringProp name="ThreadGroup.num_threads">1</stringProp>
        <stringProp name="ThreadGroup.ramp_time">1</stringProp>
        <boolProp name="ThreadGroup.scheduler">false</boolProp>
        <stringProp name="ThreadGroup.duration"></stringProp>
        <stringProp name="ThreadGroup.delay"></stringProp>
        <boolProp name="ThreadGroup.same_user_on_next_iteration">true</boolProp>
        <boolProp name="ThreadGroup.delayedStart">false</boolProp>
      </ThreadGroup>
      <hashTree>
        <!-- Echo Test Template -->
        <com.fep.jmeter.sampler.TemplateSampler guiclass="TestBeanGUI" testclass="com.fep.jmeter.sampler.TemplateSampler" testname="Echo Test" enabled="true">
          <stringProp name="targetHost">${fep_host}</stringProp>
          <intProp name="targetPort">${fep_port}</intProp>
          <intProp name="connectionTimeout">10000</intProp>
          <intProp name="readTimeout">30000</intProp>
          <stringProp name="templateName">Echo Test</stringProp>
          <stringProp name="mtiOverride"></stringProp>
          <stringProp name="amount"></stringProp>
          <stringProp name="cardNumber"></stringProp>
          <stringProp name="terminalId">TERM0001</stringProp>
          <stringProp name="bankCode">012</stringProp>
          <stringProp name="sourceAccount"></stringProp>
          <stringProp name="destAccount"></stringProp>
          <stringProp name="customFields"></stringProp>
        </com.fep.jmeter.sampler.TemplateSampler>
        <hashTree/>

        <!-- Withdrawal Template -->
        <com.fep.jmeter.sampler.TemplateSampler guiclass="TestBeanGUI" testclass="com.fep.jmeter.sampler.TemplateSampler" testname="Withdrawal Transaction" enabled="true">
          <stringProp name="targetHost">${fep_host}</stringProp>
          <intProp name="targetPort">${fep_port}</intProp>
          <intProp name="connectionTimeout">10000</intProp>
          <intProp name="readTimeout">30000</intProp>
          <stringProp name="templateName">Withdrawal</stringProp>
          <stringProp name="mtiOverride"></stringProp>
          <stringProp name="amount">100000</stringProp>
          <stringProp name="cardNumber">4111111111111111</stringProp>
          <stringProp name="terminalId">ATM00001</stringProp>
          <stringProp name="bankCode">012</stringProp>
          <stringProp name="sourceAccount"></stringProp>
          <stringProp name="destAccount"></stringProp>
          <stringProp name="customFields"></stringProp>
        </com.fep.jmeter.sampler.TemplateSampler>
        <hashTree/>

        <!-- Balance Inquiry Template -->
        <com.fep.jmeter.sampler.TemplateSampler guiclass="TestBeanGUI" testclass="com.fep.jmeter.sampler.TemplateSampler" testname="Balance Inquiry" enabled="true">
          <stringProp name="targetHost">${fep_host}</stringProp>
          <intProp name="targetPort">${fep_port}</intProp>
          <intProp name="connectionTimeout">10000</intProp>
          <intProp name="readTimeout">30000</intProp>
          <stringProp name="templateName">Balance Inquiry</stringProp>
          <stringProp name="mtiOverride"></stringProp>
          <stringProp name="amount"></stringProp>
          <stringProp name="cardNumber">4111111111111111</stringProp>
          <stringProp name="terminalId">ATM00001</stringProp>
          <stringProp name="bankCode">012</stringProp>
          <stringProp name="sourceAccount"></stringProp>
          <stringProp name="destAccount"></stringProp>
          <stringProp name="customFields"></stringProp>
        </com.fep.jmeter.sampler.TemplateSampler>
        <hashTree/>

        <!-- Fund Transfer Template -->
        <com.fep.jmeter.sampler.TemplateSampler guiclass="TestBeanGUI" testclass="com.fep.jmeter.sampler.TemplateSampler" testname="Fund Transfer" enabled="true">
          <stringProp name="targetHost">${fep_host}</stringProp>
          <intProp name="targetPort">${fep_port}</intProp>
          <intProp name="connectionTimeout">10000</intProp>
          <intProp name="readTimeout">30000</intProp>
          <stringProp name="templateName">Fund Transfer</stringProp>
          <stringProp name="mtiOverride"></stringProp>
          <stringProp name="amount">50000</stringProp>
          <stringProp name="cardNumber">4111111111111111</stringProp>
          <stringProp name="terminalId">ATM00001</stringProp>
          <stringProp name="bankCode">012</stringProp>
          <stringProp name="sourceAccount">1234567890</stringProp>
          <stringProp name="destAccount">0987654321</stringProp>
          <stringProp name="customFields"></stringProp>
        </com.fep.jmeter.sampler.TemplateSampler>
        <hashTree/>
      </hashTree>

      <!-- Custom Template Thread Group -->
      <ThreadGroup guiclass="ThreadGroupGui" testclass="ThreadGroup" testname="Custom Template Test" enabled="true">
        <stringProp name="ThreadGroup.on_sample_error">continue</stringProp>
        <elementProp name="ThreadGroup.main_controller" elementType="LoopController" guiclass="LoopControlPanel" testclass="LoopController" testname="Loop Controller" enabled="true">
          <stringProp name="LoopController.loops">1</stringProp>
          <boolProp name="LoopController.continue_forever">false</boolProp>
        </elementProp>
        <stringProp name="ThreadGroup.num_threads">1</stringProp>
        <stringProp name="ThreadGroup.ramp_time">1</stringProp>
        <boolProp name="ThreadGroup.scheduler">false</boolProp>
        <stringProp name="ThreadGroup.duration"></stringProp>
        <stringProp name="ThreadGroup.delay"></stringProp>
        <boolProp name="ThreadGroup.same_user_on_next_iteration">true</boolProp>
        <boolProp name="ThreadGroup.delayedStart">false</boolProp>
      </ThreadGroup>
      <hashTree>
        <!-- Custom Template Config (Inline) -->
        <com.fep.jmeter.config.TransactionTemplateConfig guiclass="TestBeanGUI" testclass="com.fep.jmeter.config.TransactionTemplateConfig" testname="Custom Template Config" enabled="true">
          <stringProp name="templateSource">INLINE</stringProp>
          <stringProp name="templateFile"></stringProp>
          <stringProp name="inlineTemplates">[
  {
    "name": "Custom Authorization",
    "mti": "0100",
    "description": "Pre-authorization request",
    "fields": {
      "3": "000000",
      "4": "${amount}",
      "11": "${stan}",
      "12": "${time}",
      "13": "${date}",
      "22": "051",
      "25": "00",
      "32": "${bankCode}",
      "41": "${terminalId}",
      "49": "901"
    },
    "validationRules": "REQUIRED:2,3,4,11,41",
    "enabled": true
  },
  {
    "name": "Custom Reversal",
    "mti": "0400",
    "description": "Transaction reversal",
    "fields": {
      "3": "010000",
      "4": "${amount}",
      "11": "${stan}",
      "12": "${time}",
      "13": "${date}",
      "32": "${bankCode}",
      "37": "${originalRrn}",
      "41": "${terminalId}",
      "90": "${originalDataElements}"
    },
    "validationRules": "REQUIRED:3,4,11,37,41",
    "enabled": true
  }
]</stringProp>
          <boolProp name="useCommonTemplates">true</boolProp>
          <boolProp name="autoGenerateStan">true</boolProp>
          <boolProp name="autoGenerateTimestamp">true</boolProp>
        </com.fep.jmeter.config.TransactionTemplateConfig>
        <hashTree/>

        <!-- Custom Authorization -->
        <com.fep.jmeter.sampler.TemplateSampler guiclass="TestBeanGUI" testclass="com.fep.jmeter.sampler.TemplateSampler" testname="Custom Authorization" enabled="true">
          <stringProp name="targetHost">${fep_host}</stringProp>
          <intProp name="targetPort">${fep_port}</intProp>
          <intProp name="connectionTimeout">10000</intProp>
          <intProp name="readTimeout">30000</intProp>
          <stringProp name="templateName">Custom Authorization</stringProp>
          <stringProp name="mtiOverride"></stringProp>
          <stringProp name="amount">250000</stringProp>
          <stringProp name="cardNumber">4716123456781234</stringProp>
          <stringProp name="terminalId">POS00001</stringProp>
          <stringProp name="bankCode">013</stringProp>
          <stringProp name="sourceAccount"></stringProp>
          <stringProp name="destAccount"></stringProp>
          <stringProp name="customFields"></stringProp>
        </com.fep.jmeter.sampler.TemplateSampler>
        <hashTree/>

        <!-- Using Common Template with MTI Override -->
        <com.fep.jmeter.sampler.TemplateSampler guiclass="TestBeanGUI" testclass="com.fep.jmeter.sampler.TemplateSampler" testname="Withdrawal with MTI Override" enabled="true">
          <stringProp name="targetHost">${fep_host}</stringProp>
          <intProp name="targetPort">${fep_port}</intProp>
          <intProp name="connectionTimeout">10000</intProp>
          <intProp name="readTimeout">30000</intProp>
          <stringProp name="templateName">Withdrawal</stringProp>
          <stringProp name="mtiOverride">0100</stringProp>
          <stringProp name="amount">500000</stringProp>
          <stringProp name="cardNumber">4111111111111111</stringProp>
          <stringProp name="terminalId">ATM00002</stringProp>
          <stringProp name="bankCode">012</stringProp>
          <stringProp name="sourceAccount"></stringProp>
          <stringProp name="destAccount"></stringProp>
          <stringProp name="customFields">48:Premium Customer;62:VIP</stringProp>
        </com.fep.jmeter.sampler.TemplateSampler>
        <hashTree/>
      </hashTree>

      <!-- File-Based Template Thread Group -->
      <ThreadGroup guiclass="ThreadGroupGui" testclass="ThreadGroup" testname="File-Based Template Test" enabled="false">
        <stringProp name="ThreadGroup.on_sample_error">continue</stringProp>
        <elementProp name="ThreadGroup.main_controller" elementType="LoopController" guiclass="LoopControlPanel" testclass="LoopController" testname="Loop Controller" enabled="true">
          <stringProp name="LoopController.loops">1</stringProp>
          <boolProp name="LoopController.continue_forever">false</boolProp>
        </elementProp>
        <stringProp name="ThreadGroup.num_threads">1</stringProp>
        <stringProp name="ThreadGroup.ramp_time">1</stringProp>
        <boolProp name="ThreadGroup.scheduler">false</boolProp>
        <stringProp name="ThreadGroup.duration"></stringProp>
        <stringProp name="ThreadGroup.delay"></stringProp>
        <boolProp name="ThreadGroup.same_user_on_next_iteration">true</boolProp>
        <boolProp name="ThreadGroup.delayedStart">false</boolProp>
        <stringProp name="TestPlan.comments">Enable this group and set template file path to test file-based templates</stringProp>
      </ThreadGroup>
      <hashTree>
        <!-- File-Based Template Config -->
        <com.fep.jmeter.config.TransactionTemplateConfig guiclass="TestBeanGUI" testclass="com.fep.jmeter.config.TransactionTemplateConfig" testname="File-Based Template Config" enabled="true">
          <stringProp name="templateSource">FILE</stringProp>
          <stringProp name="templateFile">/path/to/your/templates.json</stringProp>
          <stringProp name="inlineTemplates"></stringProp>
          <boolProp name="useCommonTemplates">false</boolProp>
          <boolProp name="autoGenerateStan">true</boolProp>
          <boolProp name="autoGenerateTimestamp">true</boolProp>
        </com.fep.jmeter.config.TransactionTemplateConfig>
        <hashTree/>

        <com.fep.jmeter.sampler.TemplateSampler guiclass="TestBeanGUI" testclass="com.fep.jmeter.sampler.TemplateSampler" testname="File Template Transaction" enabled="true">
          <stringProp name="targetHost">${fep_host}</stringProp>
          <intProp name="targetPort">${fep_port}</intProp>
          <intProp name="connectionTimeout">10000</intProp>
          <intProp name="readTimeout">30000</intProp>
          <stringProp name="templateName">Your Template Name</stringProp>
          <stringProp name="mtiOverride"></stringProp>
          <stringProp name="amount">100000</stringProp>
          <stringProp name="cardNumber"></stringProp>
          <stringProp name="terminalId"></stringProp>
          <stringProp name="bankCode"></stringProp>
          <stringProp name="sourceAccount"></stringProp>
          <stringProp name="destAccount"></stringProp>
          <stringProp name="customFields"></stringProp>
        </com.fep.jmeter.sampler.TemplateSampler>
        <hashTree/>
      </hashTree>

      <!-- View Results Tree -->
      <ResultCollector guiclass="ViewResultsFullVisualizer" testclass="ResultCollector" testname="View Results Tree" enabled="true">
        <boolProp name="ResultCollector.error_logging">false</boolProp>
        <objProp>
          <name>saveConfig</name>
          <value class="SampleSaveConfiguration">
            <time>true</time>
            <latency>true</latency>
            <timestamp>true</timestamp>
            <success>true</success>
            <label>true</label>
            <code>true</code>
            <message>true</message>
            <threadName>true</threadName>
            <dataType>true</dataType>
            <encoding>false</encoding>
            <assertions>true</assertions>
            <subresults>true</subresults>
            <responseData>true</responseData>
            <samplerData>true</samplerData>
            <xml>false</xml>
            <fieldNames>true</fieldNames>
            <responseHeaders>false</responseHeaders>
            <requestHeaders>false</requestHeaders>
            <responseDataOnError>false</responseDataOnError>
            <saveAssertionResultsFailureMessage>true</saveAssertionResultsFailureMessage>
            <assertionsResultsToSave>0</assertionsResultsToSave>
            <bytes>true</bytes>
            <sentBytes>true</sentBytes>
            <url>true</url>
            <threadCounts>true</threadCounts>
            <idleTime>true</idleTime>
            <connectTime>true</connectTime>
          </value>
        </objProp>
        <stringProp name="filename"></stringProp>
      </ResultCollector>
